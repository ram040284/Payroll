<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.payroll.login.dataobjects.User" table="USER_MASTER">  
    <id name="userIdPk" type="integer" >  
     <column name="USR_ID_PK" sql-type="integer" not-null="true"/>  
     <generator class="increment"/>
    </id>  
    <property name="userId" column="USR_ID"  not-null="true" />
    <property name="password" column="USR_PWD" not-null="true"/>
    <property name="empId" column="EMP_ID" type="integer" />
   <!-- <property name="roleId" column="ROLE_ID" type="integer" />  --> 
    <property name="rowUpdatedDate" column="ROW_UPD_DATE" not-null="true"/>
    <property name="status" column="STATUS" />
	<many-to-one name="role" fetch="select" 
             class="com.payroll.login.dataobjects.UserRoles"
             column="ROLE_ID"
             cascade="all" lazy="false">
	</many-to-one>
  </class>  
 </hibernate-mapping> 