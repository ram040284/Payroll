<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.payroll.employee.deductions.dataobjects.EmpDeductionDetails" table="EMP_DEDUCTION_DETAILS">  
    <id name="employeeId" type="java.lang.Integer">
			<column name="EMP_ID" />
			<generator class="foreign">
				<param name="property">employee</param>
			</generator>
	</id>
	<one-to-one name="employee" class="com.payroll.employee.dataobjects.Employee"
		constrained="true" fetch="select"></one-to-one>
	<property name="afkRent" column="AFK_RENT" />  
    <property name="society" column="SOCITY" />
    <property name="electRecovery" column="ELECT_RCVRY" />  
    <property name="courtRecovery" column="COURT_RCVRY"/> 
    <property name="unionFee" column="UNION_FEE" />  
    <property name="otherDeductions" column="OTHER_DEDUCTION" />
    <property name="miscRecovery" column="MIS_RCVRY" />  
    <property name="kssUnionRecovery" column="KSS_UNION_FEE"/>  
     <property name="monthDate" column="MONTH_DATE"/> 
    <property name="status" column="STATUS" not-null="true"/>  
    <property name="rowUpdDate" column="ROW_UPD_DATE" not-null="true"/>
  </class>  
 </hibernate-mapping> 